# All keys are loaded by app.config.js (dotenv) and exposed via extra / process.env.
# Use EXPO_PUBLIC_* for client-side; restart after changes: npx expo start

EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=
EXPO_PUBLIC_GEMINIKEY=
# Or: GEMINIKEY=  (app.config.js accepts both)

# MCP Servers (multiple supported)
# Google Workspace MCP (primary) â€” set to your deployed MCP URL; no localhost default
EXPO_PUBLIC_MCP_SERVER_URL=

# Optional: Kite MCP
# EXPO_PUBLIC_MCP_KITE_URL=https://mcp.kite.trade/mcp

# Travel MCP (self-hosted SerpAPI): flights, hotels, web search.
# URL + API key are required. Key is sent as X-API-Key header.
EXPO_PUBLIC_MCP_TRAVEL_URL=
EXPO_PUBLIC_MCP_API_KEY=

# Memory API URL (Mem0 backend with Clerk JWT auth)
# Create a JWT template in Clerk (e.g. "second-brain-jwt") and use it in services/jwt.ts (CLERK_JWT_TEMPLATE).
# The memory backend must be configured to validate that template's JWTs.
EXPO_PUBLIC_MEMORY_API_URL=

# Preferences API (MongoDB-backed profile & agent context; Clerk JWT auth)
EXPO_PUBLIC_PREFERENCES_API_URL=
